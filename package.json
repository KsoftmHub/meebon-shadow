{
  "name": "@meebon/shadow",
  "version": "1.0.0",
  "description": "api template",
  "author": "kajalanS <103587022+kajalanS@users.noreply.github.com>",
  "homepage": "https://github.com/KsoftmHub/meebon-shadow?tab=readme-ov-file#getting-started",
  "license": "ISC",
  "main": "src/index.js",
  "type": "module",
  "types": "src/types",
  "private": true,
  "directories": {
    "src": "src",
    "test": "tests",
    "lib": "lib"
  },
  "files": [
    "dist",
    "uploads"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KsoftmHub/meebon-shadow.git"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "typeorm": "typeorm-ts-node-esm",
    "rm:node": "find . -name \"node_modules\" -type d -prune -exec rm -rf '{}' +",
    "rm:dist": "find . -name \"dist\" -type d -prune -exec rm -rf '{}' +",
    "clean": "pnpm run rm:node && pnpm run rm:dist",
    "ci": "pnpm run clean && pnpm recursive install",
    "start": "node ./dist/index.js",
    "test": "jest",
    "dev": "nodemon",
    "build": "tsc --build tsconfig.json --verbose",
    "build:clean": "tsc --build --clean",
    "db:migrate": "pnpm run typeorm -- migration:run -d ./src/data-source.ts"
  },
  "bugs": {
    "url": "https://github.com/KsoftmHub/meebon-shadow/issues"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "multer": "1.4.5-lts.1",
    "mysql2": "^3.12.0",
    "path": "^0.12.7",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.5",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/lodash": "^4.17.15",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.1",
    "@types/sequelize": "^4.28.20",
    "eslint": "^9.20.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.6",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typeorm-codebase-sync": "^1.1.2",
    "typeorm-ts-node-esm": "^0.3.20",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
  }
}