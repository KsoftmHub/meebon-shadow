  "repository": {
    "type": "git",
    "url": "git+https://github.com/KsoftmHub/miller_api_template.git"
  },
  "scripts": {
    "sequelize": "sequelize",
    "rm:node": "find . -name \"node_modules\" -type d -prune -exec rm -rf '{}' +",
    "rm:dist": "find . -name \"dist\" -type d -prune -exec rm -rf '{}' +",
    "clean": "pnpm run rm:node && pnpm run rm:dist",
    "ci": "pnpm run clean && pnpm i",
    "start": "node ./dist/index.js",
    "test": "jest",
    "dev": "nodemon",
    "build": "tsc --build tsconfig.json --verbose",
    "build:clean": "tsc --build --clean"
  },
  "bugs": {
    "url": "https://github.com/KsoftmHub/miller_api_template/issues"
  },
