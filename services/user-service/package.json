{
  "name": "@my-microservices/user-service",
  "version": "1.0.0",
  "description": "User management microservice",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon dist/index.js",
    "lint": "eslint src/**/*.ts",
